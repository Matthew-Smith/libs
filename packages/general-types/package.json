    "@voiceflow/git-branch-check": "^1.2.0",
    "@voiceflow/prettier-config": "^1.0.5",
    "@zerollup/ts-transform-paths": "^1.7.9",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.0.2",
    "depcheck": "^1.3.1",
    "eslint": "^7.18.0",
    "fixpack": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
